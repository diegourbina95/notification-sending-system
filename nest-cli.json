{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/notification-sending-system/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/notification-sending-system/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/notification-sending-system",
  "projects": {
    "notification-sending-system": {
      "type": "application",
      "root": "apps/notification-sending-system",
      "entryFile": "main",
      "sourceRoot": "apps/notification-sending-system/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notification-sending-system/tsconfig.app.json"
      }
    },
    "notification-publisher": {
      "type": "application",
      "root": "apps/notification-publisher",
      "entryFile": "main",
      "sourceRoot": "apps/notification-publisher/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notification-publisher/tsconfig.app.json"
      }
    },
    "notification-consumer": {
      "type": "application",
      "root": "apps/notification-consumer",
      "entryFile": "main",
      "sourceRoot": "apps/notification-consumer/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notification-consumer/tsconfig.app.json"
      }
    }
  }
}
